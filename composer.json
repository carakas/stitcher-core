{
    "name" : "pageon/stitcher-core",
    "homepage": "http://stitcher.pageon.be",
    "description" : "Create blazing fast websites with PHP, a template engine of your choice and YAML or JSON.",
    "keywords" : [
        "static site",
        "static",
        "generator",
        "performance",
        "twig",
        "smarty",
        "SEO"
    ],
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Brent",
            "email" : "brent@pageon.be"
        }
    ],
    "prefer-stable" : true,
    "require" : {
        "PHP" : ">=7.0",
        "brendt/responsive-images" : "^1.5",
        "smarty/smarty" : "^3.1",
        "twig/twig" : "~1.0",
        "symfony/yaml" : "^3.1",
        "symfony/finder" : "^3.1",
        "symfony/filesystem" : "^3.1",
        "symfony/console" : "^3.1",
        "symfony/routing" : "^3.1",
        "symfony/dependency-injection" : "^3.1",
        "leafo/scssphp" : "^0.6.6",
        "mrclay/minify" : "^2.3",
        "erusev/parsedown" : "^1.6",
        "symfony/config" : "^3.2",
        "pageon/html-meta" : "^2.0",
        "tivie/htaccess-parser" : "^0.2@dev",
        "symfony/event-dispatcher": "^3.2"
    },
    "require-dev" : {
        "larapack/dd" : "^1.0",
        "phpunit/phpunit" : "^6.0"
    },
    "autoload" : {
        "psr-4" : {
            "Brendt\\Stitcher\\" : "src/Brendt/Stitcher"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "MyPlugin\\" : "tests/MyPlugin"
        }
    }
}
